{
    "name": "grundsalg-aarhus/grundsalg_web",
    "description": "grundsalg-aarhus/grundsalg_web",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "web/modules/custom/itk_media_entity"
        },
        {
            "type": "path",
            "url": "web/modules/custom/grundsalg_db_client"
        },
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/toolbar_visibility-3127402.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "~1.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/console": "~1.0",
        "drupal/contact_block": "^1.3",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/entityqueue": "^1.0",
        "drupal/field_group": "^3.2",
        "drupal/grundsalg_db_client": "*",
        "drupal/maxlength": "^1.0@rc",
        "drupal/metatag": "^1.19",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.7",
        "drupal/role_delegation": "^1.1",
        "drupal/system_status": "^2.8",
        "drupal/toolbar_visibility": "dev-3127402-drupal-9-readiness",
        "drupal/upgrade_status": "^2.7",
        "drupal/view_unpublished": "^1.0",
        "drush/drush": "^10.0",
        "itk-dev/itk_azure_video": "^1.1",
        "itk-dev/itk_media_entity": "*",
        "itk-dev/web_accessibility_statement": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/console-extend-plugin": true,
            "drupal-composer/drupal-scaffold": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
